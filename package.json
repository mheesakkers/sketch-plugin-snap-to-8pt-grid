{
  "name": "SnapTo8ptGrid",
  "description": "A Sketch plugin which helps with snapping layer properties to the 8 point grid.",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "SnapTo8ptGrid",
    "manifest": "src/manifest.json",
    "main": "Snap to 8pt Grid.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Misha Heesakkers <misha.heesakkers@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mheesakkers/sketch-plugin-snap-to-8pt-grid"
  }
}
